package com.company;

public class Hometask5p1{

    public static void main(String[] args)  {
        int mass[] = new int[51];
        
        for (int i=1; i<51; i++) {
            mass[i] = (i*2)-1;
        }
        
        for (int i=1; i<mass.length; i++){
            System.out.print(mass[i] + " ");
        }
        System.out.println();
        
        for (int i=50; i>=1;i--){
            System.out.print(mass[i] + " ");
        }


    }
}
